{
  "manifest_version": 2,

  "name": "duplet",
  "description": "minimalize displayed content",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "content_scripts": [
       {
         "matches": ["https://duplet.com/*"],
         "js": ["prototype.js"]
       }
     ]

}
